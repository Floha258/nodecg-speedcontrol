{
  "name": "nodecg-speedcontrol",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingothon/nodecg-speedcontrol.git"
  },
  "author": "zoton2",
  "contributors": [
    "Charleon",
    "btrim",
    "Zephyyrr",
    "Planks"
  ],
  "dependencies": {
    "async": "^2.4.1",
    "bower-npm-resolver": "^0.10.0",
    "cheerio": "^1.0.0-rc.1",
    "clone": "^2.1.1",
    "deep-equal": "^1.0.1",
    "discord.js": "^11.4.2",
    "graphql-request": "^1.6.0",
    "inherits": "^2.0.1",
    "livesplit-core": "^0.10.2",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.2",
    "moment": "^2.18.1",
    "needle": "^2.2.4",
    "nodecg-utility-obs": "^5.0.6",
    "pusher-js": "^4.3.1",
    "remove-markdown": "^0.3.0",
    "request": "^2.73.0",
    "request-promise": "^4.2.1",
    "tmi.js": "^1.1.1",
    "twitch-js": "^1.2.17",
    "ws": "^6.1.2"
  },
  "description": "NodeCG bundle to help facilitate the running of speedrunning marathons, including overlays.",
  "license": "MIT",
  "nodecg": {
    "compatibleRange": "^1.2.2",
    "graphics": [
      {
        "file": "donation-dashboard.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "bingo-board.html",
        "width": 1280,
        "height": 720
      }
    ],
    "dashboardPanels": [
      {
        "name": "obs-control",
        "title": "OBS Remote Control",
        "file": "obs.html",
        "width": 3,
        "headerColor": "#008BB9"
      },
      {
        "name": "run-player",
        "title": "Run Player",
        "width": 3,
        "file": "RunPlayerDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "run-control",
        "title": "Run Editor",
        "width": 3,
        "file": "RunControlDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "player-control",
        "title": "Add/Remove Runs",
        "width": 2,
        "file": "PlayerControlDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "run-timer",
        "title": "Run Timer",
        "width": 2,
        "file": "PlayerTimerDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "player-layout",
        "title": "Player Layout",
        "width": 2,
        "file": "PlayerLayoutDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Scene Editing"
      },
      {
        "name": "tiltify-dashboard",
        "title": "Tiltify Dashboard",
        "width": 2,
        "file": "TiltifyDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "twitch-player-control",
        "title": "Twitch Player",
        "width": 2,
        "file": "TwitchPlayerControl.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "bingo-color-chooser",
        "title": "Bingo Color Chooser",
        "width": 2,
        "file": "BingoColorChooser.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "stream-control",
        "title": "Twitch Control",
        "width": 3,
        "file": "StreamControlDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "horaro-import",
        "title": "Horaro Schedule Import",
        "width": 2,
        "file": "HoraroImportDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "twitch-resizer-dashboard",
        "title": "Twitch Resizer",
        "width": 10,
        "file": "TwitchResizerDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Twitch Resizing"
      },
      {
        "name": "run-info",
        "title": "Add/Edit Run Info",
        "width": 5,
        "file": "RunInfoDialog.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "add/edit run",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ]
      }
    ]
  }
}
